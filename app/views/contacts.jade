ul.wh-contacts(ng-controller="ContactsCtrl", ng-show="contacts.length")
    li(ng-repeat="contact in contacts")
        a.wh-contact(ng-href="{{contact.type=='phone'?'tel:':contact.type=='mail'?'mailto:':''}}{{contact.value}}",title="{{contact.value}}",data-wh-icon="{{contact.icon}}",ng-class="contact.type") {{contact.value}}

